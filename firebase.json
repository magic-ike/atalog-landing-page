{
  "hosting": {
      
    "public": "public",
    
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "public-old/",
      "templates/"
    ],
    
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ],
    
    "cleanUrls": true,

    "trailingSlash": false
    
  }
}
